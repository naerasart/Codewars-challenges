 //Sum Mixed Array

/* Given an array of integers as strings and numbers, return the sum of the array values as if 

all were numbers.

Return your answer as a number.*/


//Answer:
function sumMix(x){
   
let result = x.map(a=> +a);
console.log(result);
  var y =0;
 for (let i=0; i<result.length; i++){
 y= result.reduce(
  (previousValue, currentValue) => previousValue + currentValue,
 y
);

  console.log(y);
   return y;
   
   }

//Another Answer:

function sumMix(x){
   
let result = x.map(a=> +a);
console.log(result);
  var y =0;
  var z=0;
 for (let i=0; i<result.length; i++){
   z=i++;
 y= result.reduce(
  (i, z) => i + z,
 y
);
   
    
  
   
}
